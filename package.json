  "scripts": {
    "dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on http://127.0.0.1:3005 --timeout=300000 && electron .",
    "codegen": "graphql-codegen && node ./src/types/generateFlatTypes.js",
    "start": "cross-env PORT=3005 craco start",
    "build": "craco build && electron-builder build --mac --windows --publish never",
    "deploy": "craco build && git remote set-url origin https://github.com/adileoca/electron-releases.git && electron-builder build --windows --mac --publish always && git remote set-url origin https://github.com/adileoca/ceramica-interface.git",
    "test": "craco test",
    "eject": "react-scripts eject",
    "dist": "electron-builder --publish=never",
    "lint": "eslint . --ext .ts,.tsx",
    "db:types": "npx supabase gen types typescript --project-id vrdaoudvtphptybaljqq > src/lib/supabase/database.types.ts"
