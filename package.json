{
  "name": "operations-manager",
  "version": "0.1.0",
  "main": "src/core/main.js",
  "private": true,
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "https://github.com/https://github.com/adileoca/electron-releases.git"
  },
  "build": {
    "appId": "com.operationsmanager",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "protocols": [
      {
        "name": "OperationsManager",
        "schemes": [
          "com.operationsmanager"
        ]
      }
    ]
  },
  "scripts": {
    "package": "craco build && npx electron-packager . YourAppName --platform=darwin --arch=x64 --icon=assets/Icons.icns --out=out --overwrite",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on http://127.0.0.1:3001 --timeout=30000 && electron .",
    "codegen": "graphql-codegen && node ./src/types/generateFlatTypes.js",
    "start": "PORT=3001 craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "dist": "electron-builder --publish=never",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@apollo/client": "^3.10.3",
    "@auth0/auth0-react": "^2.2.4",
    "@aws-sdk/client-s3": "^3.717.0",
    "@headlessui/react": "^2",
    "@heroicons/react": "^2.1.3",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@supabase/supabase-js": "^2.45.4",
    "@types/glob": "^8.1.0",
    "@types/minimatch": "^5.1.2",
    "animejs": "^3.2.2",
    "axios": "^1.6.8",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "crpyto-js": "^0.0.1-security",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "dompurify": "^3.1.7",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.9",
    "express": "^4.19.2",
    "flag-icons": "^6.15.0",
    "graphql": "^16.8.1",
    "lucide-react": "^0.441.0",
    "mailparser": "^3.7.1",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "p-limit": "^6.1.0",
    "p-queue": "^8.0.1",
    "react": "^18.3.1",
    "react-day-picker": "^9.5.0",
    "react-dom": "^18.3.1",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.12.7",
    "subscriptions-transport-ws": "^0.11.0",
    "uuid": "^11.0.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@craco/craco": "^7.1.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-react-apollo": "^4.3.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/animejs": "^3.1.12",
    "@types/dompurify": "^3.0.5",
    "@types/electron": "^1.6.10",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.12.10",
    "@types/react": "^18.2.79",
    "@types/react-resizable": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "@typescript-eslint/parser": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "concurrently": "^7.6.0",
    "electron": "^21.4.4",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "tailwindcss": "^3.4.3",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {}
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
